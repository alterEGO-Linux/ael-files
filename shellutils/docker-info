# /usr/bin/env sh
# ------------------------------------------------------------------------ INFO
# [shellutils/docker-info]
# author        : Pascal Malouin @https://github.com/alterEGO-Linux
# created       : 2025-09-04 10:19:18 UTC
# updated       : 2025-12-29 00:37:23 UTC
# description   : Docker status helper.

[ -n "${__SOURCED_DOCKER_INFO:-}" ] && return
__SOURCED_DOCKER_INFO=1

docker-info() {
    command python ${HOME}/.ael/bin/docker-info.py "${@}"
}
